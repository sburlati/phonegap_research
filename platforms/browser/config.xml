<?xml version='1.0' encoding='utf-8'?>
<widget id="com.sburlati.phonegap_research" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>phonegap_research</name>
    <description>
        phonegap_research: A basic Framework7 template for PhoneGap.
    </description>
    <author email="marco@sburlati.com" href="http://win.sburlati.com">
        Phonegap Research - from an example of Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
